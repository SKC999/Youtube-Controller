{
  "expo": {
    "name": "YouTube Controller",
    "slug": "youtube-controller",
    "scheme": "youtube-controller",
    "sdkVersion": "53.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.locusflow.youtubecontroller",
      "buildNumber": "24",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "youtube-controller",
            "CFBundleURLSchemes": [
              "youtube-controller"
            ]
          },
          {
            "CFBundleURLName": "google-oauth-ios",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.398239762640-pcssb2kt1sf9ivsfmuouguiho27o8ssh"
            ]
          },
          {
            "CFBundleURLName": "youtube-controller",
            "CFBundleURLSchemes": [
              "youtube-controller"
            ]
          },
          {
            "CFBundleURLName": "google-oauth-ios",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.398239762640-pcssb2kt1sf9ivsfmuouguiho27o8ssh"
            ]
          }
        ],
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "NSPhotoLibraryUsageDescription": "This app does not access your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.locusflow.youtubecontroller",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.SYSTEM_ALERT_WINDOW"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "youtube-controller"
            },
            {
              "scheme": "com.googleusercontent.apps.398239762640-pcssb2kt1sf9ivsfmuouguiho27o8ssh"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-dev-client",
      "expo-secure-store"
    ],
    "linking": {
      "scheme": "youtube-controller",
      "prefixes": [
        "youtube-controller://",
        "com.googleusercontent.apps.398239762640-pcssb2kt1sf9ivsfmuouguiho27o8ssh://",
        "https://youtube-controller.app"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "736bcf92-94da-4e15-bf32-aabba2d5ac37"
      }
    },
    "owner": "locusflow"
  }
}
